{
  "apprehend_*": {
    "entry_*": {
      "new_filename": "string",  // REQUIRED: folder name for output
      
      "start_search_with": "swept_candle | swing_candle",
      
      "start_search_with_tool": {
        "tool": "horizontal_line | dashed_horizontal_line | box",
        "swing_higher_high_or_lower_high": "high_price | low_price",
        "swing_lower_low_or_higher_low": "high_price | low_price",
        "stop_at_its": "liquidator | null"
      },
      
      "option": "swept_candle | swept_candle_opposite | swing_candle | swing_candle_opposite",
      
      "option_condition": "default | after_start",
      
      "option_tool": {
        "tool": "horizontal_line | dashed_horizontal_line | box",
        "swing_higher_high_or_lower_high": "high_price | low_price",
        "swing_lower_low_or_higher_low": "high_price | low_price",
        "stop_at_its": "liquidator | null"
      },
      
      "find_entry_after": "swept_candle | acting_sweeper | original_sweeper | option | swing_candle",
      
      "before_entry": {
        "data_selection": "all | extreme | non_extreme",
        "communicate_only2_swings": "swing_N_and_swing_M",  // e.g., "swing_1_and_swing_2"
        "swings_communication_condition": "follow_rules | rules_or_opposite",
        
        "swing_1": {
          "swing": "sweeper_candle | sweeper_candle_opposite | sweeper_candle_identical | swept_candle | swept_candle_opposite | swept_candle_identical | swing_N | swing_N_opposite | swing_N_identical | option | swing_candle | swept_candle",
          "condition": "beyond_option | beyond_sweeper | beyond_swing_N | beyond_swing_candle | beyond_swept_candle | behind_option | behind_sweeper | behind_swing_N | behind_swing_candle | behind_swept_candle",
          "if_outlaw": "check_further | terminate",
          "if_intruder": "skip | terminate",
          
          "drawing": {
            "tool": "horizontal_line | dashed_horizontal_line | box",
            "swing_higher_high_or_lower_high": "high_price | low_price",
            "swing_lower_low_or_higher_low": "high_price | low_price",
            "stop_at_its": "liquidator | null"
          }
        },
        
        "swing_2": {
          // Same structure as swing_1
        },
        
        "swing_*": {
          // Same structure for additional swing conditions
        }
      },
      
      "subject": {
        "poi": "swept_candle | swing_candle | option | sweeper_candle | swing_N_candle",
        "if_intruder": "skip | terminate",
        
        "drawing": {
          "tool": "box | horizontal_line | dashed_horizontal_line",
          "swing_higher_high_or_lower_high": "high_price | low_price",
          "swing_lower_low_or_higher_low": "high_price | low_price"
        }
      },
      
      "record_prices": {
        "entry": {
          "record": "subject | sweeper_candle | swept_candle | swing_N | option | swing_candle",
          "swing_higher_high_or_lower_high": "high_price | low_price",
          "swing_lower_low_or_higher_low": "high_price | low_price"
        },
        
        "exit": {
          "record": "subject | sweeper_candle | swept_candle | swing_N | option | swing_candle",
          "swing_higher_high_or_lower_high": "high_price | low_price",
          "swing_lower_low_or_higher_low": "high_price | low_price"
        },
        
        "target": {
          "record": "subject | sweeper_candle | swept_candle | swing_N | option | swing_candle",
          "swing_higher_high_or_lower_high": "high_price | low_price",
          "swing_lower_low_or_higher_low": "high_price | low_price"
        },
        
        "order_type_if_entry_swing_is": {
          "swing_higher_high_or_lower_high": "buy_limit | sell_limit | buy_stop | sell_stop",
          "swing_lower_low_or_higher_low": "buy_limit | sell_limit | buy_stop | sell_stop"
        }
      },
      
      "save_to": {
        "new_filename_folder": {
          "save_charts": {
            "folder": "chart"
          },
          "save_data": {
            "pending_folder": "limit_orders",
            "mitigated_folder": "executed_orders"
          }
        }
      },
      
      "process_second_source_files": "yes | no"
    }
  }
}